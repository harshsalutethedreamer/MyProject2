<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <padding android:left="10dp"
                     android:top="5dp"
                     android:right="10dp"
                     android:bottom="5dp" />
        </shape>
    </item>
    <item android:bottom="-5dp" android:left="-5dp" android:drawable="@drawable/border_shape" android:gravity="bottom|left">
    </item>
    <item android:bottom="-5dp" android:left="-5dp" android:drawable="@drawable/border_shape2" android:gravity="bottom|left">
    </item>
    <item android:top="-5dp" android:left="-5dp" android:drawable="@drawable/border_shape" android:gravity="top|left">
    </item>
    <item android:top="-5dp" android:left="-5dp" android:drawable="@drawable/border_shape2" android:gravity="top|left">
    </item>
    <item android:bottom="-5dp" android:right="-5dp" android:drawable="@drawable/border_shape" android:gravity="bottom|right">
    </item>
    <item android:bottom="-5dp" android:right="-5dp" android:drawable="@drawable/border_shape2" android:gravity="bottom|right">
    </item>
    <item android:top="-5dp" android:right="-5dp" android:drawable="@drawable/border_shape" android:gravity="top|right">
    </item>
    <item android:top="-5dp" android:right="-5dp" android:drawable="@drawable/border_shape2" android:gravity="top|right">
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#B3B3B3"/>
            <padding android:left="10dp"
                     android:top="5dp"
                     android:right="10dp"
                     android:bottom="5dp" />
        </shape>
    </item>
</layer-list>